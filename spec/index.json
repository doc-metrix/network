  "net.dropRX": {
    "displayName": "Dropped Packets (Receiving)",
    "units": "packets",
    "max": "MAX_UNSIGNED_LONG",
    "min": 0,
    "sigFigs": null,
    "dataType": "count",
    "type": "raw",
    "formula": {
      "equation": "n_{DroppedPacketsRX} + n_{MissingPacketsRX}",
      "variables": {
        "n_{DroppedPacketsRX}": {
          "description": "Number of received packets dropped by this interface"
        },
