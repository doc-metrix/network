  "net.utilizationRX": {
    "displayName": "Average Network Utilization (Receiving)",
    "units": "utilization",
    "max": 1,
    "min": 0,
    "sigFigs": null,
    "dataType": "percentage",
    "type": "derived",
    "formula": {
      "equation": "\\frac{1}{n_{net \\neq lo}}\\sum_{net \\neq lo}\\frac{\\Delta d_{rx}}{BW \\cdot \\Delta t}",
      "variables": {
        "n_{net \\neq lo}}": {
          "description": "Number of network interfaces, excluding the loopback interface"
        },
        "\\Delta d_{rx}": {
          "description": "Amount of data received in measured time period"
        },
        "BW": {
          "description": "Bandwidth of network interface"
        },
        "\\Delta t": {
          "description": "Amount of time measured"
        }
      }
    },
    "description": "Average percentage of the maximum bandwidth across all interfaces (except loopback) being used for receiving packets.",
    "notes": "The data received is reported in bytes while bandwidth is in megabits per second, so some conversion is necessary to make the units match up.",
    "platforms": {
      "linux": ["/proc/net/dev", "ethtool"]
    },
    "device": null,
    "vendors": "*",
    "classification": "network",
    "refs": [
      "http://linux.die.net/man/5/proc",
      "http://linux.die.net/man/8/ethtool",
      "http://www.hp.com/rnd/device_help/help/hpwnd/webhelp/HPJ3298A/utilization.htm"
    ]
  },
  "net.utilizationTX": {
    "displayName": "Average Network Utilization (Transmitting)",
    "units": "utilization",
    "max": 1,
    "min": 0,
    "sigFigs": null,
    "dataType": "percentage",
    "type": "derived",
    "formula": {
      "equation": "\\frac{1}{n_{net \\neq lo}}\\sum_{net \\neq lo}\\frac{d_{tx|t=1s} - d_{tx|t=0s}}{BW}",
      "variables": {
        "n_{net \\neq lo}": {
          "description": "Number of network interfaces, excluding the loopback interface"
        },
        "\\Delta d_{tx}": {
          "description": "Amount of data transmitted in measured time period"
        },
        "BW": {
          "description": "Bandwidth of network interface"
        },
        "\\Delta t": {
          "description": "Amount of time measured"
        }
      }
    },
    "description": "Average percentage of the maximum bandwidth across all interfaces (except loopback) being used for transmitting packets.",
    "notes": "The data transmitted is reported in bytes while bandwidth is in megabits per second, so some conversion is necessary to make the units match up.",
    "platforms": {
      "linux": ["/proc/net/dev", "ethtool"]
    },
    "device": null,
    "vendors": "*",
    "classification": "network",
    "refs": [
